// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}

model Ramais {
  id      Int    @id @default(autoincrement())
  org     String
  unidade String
  setor   String
  user    String
  ramal   String
}
